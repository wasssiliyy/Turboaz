<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--#region Gradient Color-->
    <LinearGradientBrush x:Key="cs_Background" StartPoint="0,0" EndPoint="0,5">
        <GradientStop Color="#000" Offset="0.0"/>
        <GradientStop Color="#FFF5ECEB" Offset="0.5"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="cs_PopupBackground" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="#FFFF" Offset="0.0"/>
        <GradientStop Color="#FFFF" Offset="1.2"/>
    </LinearGradientBrush>
    <!--#endregion-->

    <!--#region Solid Color-->
    <SolidColorBrush x:Key="cs_BackgroundLayer" Color="#FFF5ECEB" Opacity="0.2"/>

    <SolidColorBrush x:Key="cs_BorderBrush" Color="#000"/>

    <SolidColorBrush x:Key="cs_Clouds" Color="#000"/>

    <SolidColorBrush x:Key="cs_Watermark" Color="#FFC2BBBA"/>

    <SolidColorBrush x:Key="cs_PinkSoft" Color="Red"/>

    <SolidColorBrush x:Key="cs_TextColor" Color="#FF333333"/>

    <SolidColorBrush x:Key="cs_RBBackground" Color="#000"/>
    <!--#endregion-->
</ResourceDictionary>